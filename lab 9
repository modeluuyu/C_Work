#include <stdio.h>

int main() {

    int n ;

    printf("Enter n : " ) ;
    scanf("%d",&n ) ;
    int A[n] ;

    for(int i = 0 ; i < n ; i ++ ) {
        printf("Enter A[%d] " , i ) ;
        scanf("%d",&A[i] ) ;
    }//end for
    printf("Index: " ) ;
    for(int j = 0 ; j < n ; j ++ ) {
        printf("%d ", j ) ;
    }//end for
    printf("\nArray: " ) ;
        for(int i = 0 ; i < n ; i ++ ) {
            if(A[i] / A[i] == 1 && A[i] != 1) {
                if(A[i] %2 != 0 && A[i] %3 != 0 ) {
                    printf("%d ", A[i] ) ;
                } else if(A[i] == 2 || A[i] == 3 ) {
                    printf("%d ", A[i] ) ;
                } else {
                    printf("# " ) ;
                }//end else
            } else {
                printf("# " ) ;
            }//end else
        }//end for
    return 0 ;
}//end main function
